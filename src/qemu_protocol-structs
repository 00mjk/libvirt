/* -*- c -*- */
struct remote_nonnull_domain {
        remote_nonnull_string      name;
        remote_uuid                uuid;
        int                        id;
};
struct qemu_monitor_command_args {
        remote_nonnull_domain      dom;
        remote_nonnull_string      cmd;
        int                        flags;
};
struct qemu_monitor_command_ret {
        remote_nonnull_string      result;
};
